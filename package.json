{
  "name": "english-school-games",
  "version": "1.0.0",
  "description": "English School Games Portal",
  "main": "index.js",
  "scripts": {
    "install": "npx tsc && node dist/install-games.js",
    "update": "npx tsc && node dist/update-games.js",
    "build": "npx tsc && node dist/update-games.js && node dist/build-games.js",
    "all": "npx tsc && node dist/install-games.js && node dist/update-games.js && node dist/build-games.js && node dist/index.js",
    "start": "npx tsc && node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\""
  },
  "keywords": [
    "english",
    "school",
    "games",
    "portal"
  ],
  "author": "Rodrigo Coelho <rodrigocoeio@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.2",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.5"
  }
}
